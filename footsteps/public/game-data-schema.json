{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "ChallengeGameDisplay": {
            "properties": {
                "backgroundStyle": {
                    "$ref": "#/definitions/React.CSSProperties"
                },
                "backgroundTemplate": {
                    "$ref": "#/definitions/GameDisplayTemplate"
                },
                "data": {
                    "$ref": "#/definitions/Record%3Cstring%2Cunknown%3E"
                },
                "foregroundTemplate": {
                    "$ref": "#/definitions/GameDisplayTemplate"
                },
                "type": {
                    "$ref": "#/definitions/ChallengeGameDisplayType"
                }
            },
            "required": [
                "data",
                "type"
            ],
            "type": "object"
        },
        "ChallengeGameDisplayType": {
            "enum": [
                "challenge-completed",
                "challenge-in-progress"
            ],
            "type": "string"
        },
        "GameChallengeData": {
            "properties": {
                "displays": {
                    "items": {
                        "$ref": "#/definitions/ChallengeGameDisplay"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "solution": {
                    "type": "string"
                }
            },
            "required": [
                "displays",
                "id",
                "solution"
            ],
            "type": "object"
        },
        "GameDisplayTemplate": {
            "properties": {
                "content": {
                    "type": "string"
                },
                "sourceType": {
                    "$ref": "#/definitions/TemplateSourceType"
                },
                "templateSource": {
                    "type": "string"
                }
            },
            "required": [
                "sourceType"
            ],
            "type": "object"
        },
        "OverviewGameDisplay": {
            "properties": {
                "backgroundStyle": {
                    "$ref": "#/definitions/React.CSSProperties"
                },
                "backgroundTemplate": {
                    "$ref": "#/definitions/GameDisplayTemplate"
                },
                "data": {
                    "$ref": "#/definitions/Record%3Cstring%2Cunknown%3E"
                },
                "foregroundTemplate": {
                    "$ref": "#/definitions/GameDisplayTemplate"
                },
                "type": {
                    "const": "game-overview",
                    "type": "string"
                }
            },
            "required": [
                "data",
                "type"
            ],
            "type": "object"
        },
        "React.CSSProperties": {
            "type": "object"
        },
        "Record<string,unknown>": {
            "type": "object"
        },
        "TemplateSourceType": {
            "enum": [
                "embedded",
                "relative",
                "url"
            ],
            "type": "string"
        }
    },
    "properties": {
        "challenges": {
            "items": {
                "$ref": "#/definitions/GameChallengeData"
            },
            "type": "array"
        },
        "displays": {
            "properties": {
                "overview": {
                    "$ref": "#/definitions/OverviewGameDisplay"
                }
            },
            "required": [
                "overview"
            ],
            "type": "object"
        },
        "gameContainerStyle": {
            "$ref": "#/definitions/React.CSSProperties"
        },
        "id": {
            "type": "string"
        },
        "title": {
            "type": "string"
        }
    },
    "required": [
        "challenges",
        "displays",
        "gameContainerStyle",
        "id",
        "title"
    ],
    "type": "object"
}

